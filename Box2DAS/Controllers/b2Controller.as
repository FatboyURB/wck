package Box2DAS.Controllers {		import Box2DAS.*;	import Box2DAS.Collision.*;	import Box2DAS.Collision.Shapes.*;	import Box2DAS.Common.*;	import Box2DAS.Dynamics.*;	import Box2DAS.Dynamics.Contacts.*;	import Box2DAS.Dynamics.Joints.*;	import cmodule.Box2D.*;	import flash.utils.*;	public class b2Controller {				public var effect:b2Effect;				public var storage:Dictionary = new Dictionary();				public function b2Controller(e:b2Effect = null) {			effect = e;		}				public function toString():String {			return GetBodies().toString();		}				public function GetBodies():Array {			var b:Array = [];			for(var i:* in storage) {				b.push(i);			}			return b;		}				public function AddBody(body:b2Body):void {			storage[body] = true;		}				public function RemoveBody(body:b2Body):void {			delete storage[body];		}				public function Step():void {			if(effect) {				for(var i:* in storage) {					effect.Apply(i as b2Body);				}			}		}	}}