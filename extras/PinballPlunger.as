package extras {		import Box2DAS.*;	import Box2DAS.Collision.*;	import Box2DAS.Collision.Shapes.*;	import Box2DAS.Common.*;	import Box2DAS.Dynamics.*;	import Box2DAS.Dynamics.Contacts.*;	import Box2DAS.Dynamics.Joints.*;	import cmodule.Box2D.*;	import wck.*;	import shapes.*;	import misc.*;	import extras.*;	import flash.utils.*;	import flash.events.*;	import flash.display.*;	import flash.text.*;	import flash.geom.*;		/**	 *	 */	public class PinballPlunger extends Entity {				public var joint:Joint;				public override function create():void {			super.create();			joint = getChildByName('j') as Joint;			listenWhileVisible(stage, Event.ENTER_FRAME, parseInput, false, 1);		}				public function parseInput(e:Event):void {			joint.motorSpeed = Input.kd(' ', 'DOWN') ? -5 : 70;		}	}}