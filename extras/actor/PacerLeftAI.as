package extras.actor {		import Box2DAS.*;	import Box2DAS.Collision.*;	import Box2DAS.Collision.Shapes.*;	import Box2DAS.Common.*;	import Box2DAS.Dynamics.*;	import Box2DAS.Dynamics.Contacts.*;	import Box2DAS.Dynamics.Joints.*;	import cmodule.Box2D.*;	import wck.*;	import shapes.*;	import misc.*;	import extras.*;	import extras.actor.*;	import flash.utils.*;	import flash.events.*;	import flash.display.*;	import flash.text.*;	import flash.geom.*;		public class PacerLeftAI extends ActorAI {				public var dir:String = 'left';				public override function control():void {			if(dir == 'left' && a.g.leftDot > 0.3) {				dir = 'right';			}			else if(dir == 'right' && a.g.rightDot > 0.3) {				dir = 'left';			}			a.move(dir == 'left' ? -1 : 1);		}	}}